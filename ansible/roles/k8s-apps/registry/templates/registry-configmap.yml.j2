apiVersion: v1
kind: ConfigMap
metadata:
  name: registry-config
  namespace: registry
data:
  registry.crt: |
{{ registry_crt_content | indent(width=4, indentfirst=True) }}

---
apiVersion: v1
kind: Secret
metadata:
  name: registry-secrets
  namespace: registry
data:
  htpasswd: {{ htpasswd_content }}
  registry.key: {{ registry_key_content }}
